#import <Foundation/Foundation.h>

@interface WMGoodsFavModel : NSObject

/*商品收藏的model类*/

@property(nonatomic,copy)NSString *goodsId;//商品ID
@property(nonatomic,copy)NSString *goodsImage;//商品图片
@property(nonatomic,copy)NSString *goodsName;//商品名称
@property(nonatomic,copy)NSString *goodsPrice;//商品价格
@property(nonatomic,copy)NSString *salesTerritory;//销售区域

@property(nonatomic,copy)NSString *isNewGoods;//是否新品
@property(nonatomic,copy)NSString *isGroom;//是否推荐
@property(nonatomic,copy)NSString *isSpecimen;//是否样品
@property(nonatomic,copy)NSString *isUsingIntegral;//是否可使用积分

@property(nonatomic,copy)NSString *isUsingCoupons;//是否可使用优惠券
