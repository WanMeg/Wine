#import <Foundation/Foundation.h>
#import "WMMallShop.h"


@interface WMShopDetail : NSObject



@property (nonatomic, strong) WMMallShop *mallShop;//店铺信息

@property (nonatomic, assign) BOOL isCollect;//是否收藏


/*
 状态
 Just     行业大于店铺
 negative 行业低于店铺
 equal    行业店铺相等 */
@property (nonatomic, copy) NSString *serviceAttitudeStatus;//服务态度
@property (nonatomic, copy) NSString *commodityQualityStatus;//商品质量
@property (nonatomic, copy) NSString *deliveryTimeStatus;//物流速度
@property (nonatomic, copy) NSString *serviceAttitude;//店铺服务
@property (nonatomic, copy) NSString *deliveryTime;//物流服务
@property (nonatomic, copy) NSString *commodityQuality;//商品质量
@property (nonatomic,  assign) int serviceAttitudePercentage;//服务百分比
@property (nonatomic, assign) int commodityQualityPercentage;//质量百分比
@property (nonatomic,  assign) int deliveryTimePercentage;//物流百分比



@end
