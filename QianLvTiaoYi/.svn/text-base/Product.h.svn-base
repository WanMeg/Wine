#import <Foundation/Foundation.h>

@interface Product : NSObject

@property (assign, nonatomic) float activityPrice;
@property (copy, nonatomic) NSString *allStock;
@property (copy, nonatomic) NSString *barCode;
@property (copy, nonatomic) NSString *costPrice;
@property (copy, nonatomic) NSString *goodsId;
@property (copy, nonatomic) NSString *goodsNo;
@property (copy, nonatomic) NSString *goodsNumber;
@property (copy, nonatomic) NSString *goodsProductId;
@property (copy, nonatomic) NSString *lockStock;
@property (assign, nonatomic) float marketPrice;
@property (assign, nonatomic) float postPrice;
@property (copy, nonatomic) NSString *qrCode;
@property (assign, nonatomic) float rebatePrice;

@property (copy, nonatomic) NSString *salesStock;
@property (copy, nonatomic) NSString *specNumber;
@property (copy, nonatomic) NSString *specValue;
@property (copy, nonatomic) NSString *weight;
@property (assign, nonatomic) float wholesalePrice;
@property (assign, nonatomic) float retailPrice;

@property (nonatomic, assign) BOOL isPicked;

@end


