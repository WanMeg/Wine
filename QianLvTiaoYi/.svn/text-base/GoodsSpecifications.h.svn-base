#import <Foundation/Foundation.h>
/**
 *  产品规格名称模型
 */
@interface GoodsSpecifications : NSObject
@property (nonatomic, copy) NSString *codeNo;
@property (nonatomic, copy) NSString *productSpecificationsId;
@property (nonatomic, strong) NSArray *productSpecificationsValues;
@property (nonatomic, copy) NSString *specificationsName;
@property (nonatomic, copy) NSString *specstatus;

@end


/**
 *  产品规格值模型
 */
@interface ProductSpecificationsValue : NSObject
@property (nonatomic, copy) NSString *codeNo;
@property (nonatomic, copy) NSString *productSpecificationsValueId;
@property (nonatomic, copy) NSString *specificationsValue;

@end
